{
  "name": "vitron",
  "productName": "Vitron",
  "private": false,
  "version": "0.0.9",
  "description": "Supercharged Starter",
  "author": "Yeon a.k.a Blade <dev@yeonv.com>",
  "repository": {
    "url": "https://github.com/YeonV/vitron"
  },
  "license": "MIT",
  "main": "dist/main/index.cjs",
  "scripts": {
    "start": "node scripts/start.mjs",
    "build": "tsc --noEmit -p packages/renderer/tsconfig.json && node scripts/build.mjs",
    "dev": "node scripts/watch.mjs",
    "dist": "tsc --noEmit -p packages/renderer/tsconfig.json && node scripts/build.mjs && yarn electron-builder"
  },
  "engines": {
    "node": ">=14.17.0"
  },
  "dependencies": {
    "@babel/core": "^7.20.12",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.3",
    "custom-electron-titlebar": "^4.1.5",
    "electron-devtools-installer": "^3.2.0",
    "electron-store": "^8.1.0",
    "immer": "^9.0.17",
    "react-router-dom": "6.6.1",
    "zustand": "^4.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@types/reactour": "^1.18.2",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "@vitejs/plugin-react": "^3.0.1",
    "electron": "^22.0.0",
    "electron-builder": "^23.6.0",
    "eslint": "^8.31.0",
    "eslint-plugin-react": "^7.31.11",
    "execa": "^6.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.57.1",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "vite-plugin-esmodule": "^1.4.4",
    "vite-plugin-optimizer": "^1.4.2"
  },
  "env": {
    "VITE_DEV_SERVER_HOST": "127.0.0.1",
    "VITE_DEV_SERVER_PORT": 3000,
    "VITRON_TOURS": true,
    "VITRON_PRIMARY_COLOR": "default",
    "VITRON_CUSTOM_TITLEBAR": true,
    "VITRON_SAVE_WINDOWSIZE": true,
    "VITRON_TRAY": true
  }
}
