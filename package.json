{
  "name": "vitron",
  "productName": "Vitron",
  "private": true,
  "version": "0.0.7",
  "description": "Supercharged Starter",
  "author": "Yeon a.k.a Blade <dev@yeonv.com>",
  "repository": {
    "url": "https://github.com/YeonV/vitron"
  },
  "license": "MIT",
  "main": "dist/main/index.cjs",
  "scripts": {
    "start": "node scripts/start.mjs",
    "build": "tsc --noEmit -p packages/renderer/tsconfig.json && node scripts/build.mjs",
    "dev": "node scripts/watch.mjs",
    "dist": "tsc --noEmit -p packages/renderer/tsconfig.json && node scripts/build.mjs && yarn electron-builder"
  },
  "engines": {
    "node": ">=14.17.0"
  },
  "dependencies": {
    "@babel/core": "^7.19.1",
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@mui/icons-material": "^5.10.6",
    "@mui/material": "^5.10.6",
    "custom-electron-titlebar": "^4.1.0",
    "electron-devtools-installer": "^3.2.0",
    "electron-store": "^8.1.0",
    "immer": "^9.0.15",
    "react-router-dom": "6.4.1",
    "zustand": "^4.1.1"
  },
  "devDependencies": {
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/reactour": "^1.18.2",
    "@vitejs/plugin-react": "^2.1.0",
    "electron": "^20.2.0",
    "electron-builder": "^23.3.3",
    "execa": "^6.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.55.0",
    "typescript": "^4.8.3",
    "vite": "^3.1.3",
    "vite-plugin-esmodule": "^1.4.2",
    "vite-plugin-optimizer": "^1.4.0"
  },
  "env": {
    "VITE_DEV_SERVER_HOST": "127.0.0.1",
    "VITE_DEV_SERVER_PORT": 3000,
    "VITRON_TOURS": true,
    "VITRON_PRIMARY_COLOR": "default",
    "VITRON_CUSTOM_TITLEBAR": true,
    "VITRON_SAVE_WINDOWSIZE": true,
    "VITRON_TRAY": true
  }
}
